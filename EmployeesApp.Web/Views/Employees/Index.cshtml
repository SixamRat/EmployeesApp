@model Employee[]

@{
	ViewBag.Title = "Home";
	ViewBag.Header = "Employees";
}

<ul>
	@foreach (var item in Model)
	{
		<li>
			<div class="list-row">
				<a asp-controller="Employees" asp-action="Details" asp-route-id="@item.Id">@item.Name</a>
				<partial name="_ShiftClockStamp" model="item" />
			</div>
		</li>
	}
</ul>
